{
  "root": "dist/",
  "clean_urls": true,
  "routes": {
    "/**": "index.html"
  },
  "proxies": {
    "/api/": {
      "origin": "${API_URL}/api/"
    },
    "/socket.io/": {
      "origin": "${API_URL}/socket.io/",
      "websocket": true
    }
  },
  "headers": {
    "/**": {
      "Cache-Control": "public, max-age=0, must-revalidate"
    },
    "/assets/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    }
  }
}